<?xml version="1.0" encoding="utf-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
    <!--  General attributes  -->
    <key attr.name="Label" attr.type="string" id="d1" />

    <!--  Nodes' attributes  -->
    <key attr.name="NodeType" attr.type="string" for="node" id="d2" />
    <key attr.name="CPUcap" attr.type="int" for="node" id="d3" />
    <key attr.name="RAMcap" attr.type="int" for="node" id="d4" />

    <!--  Links' attributes  -->
    <key attr.name="BWcap" attr.type="int" for="edge" id="d5" />
    <key attr.name="Latency" attr.type="int" for="edge" id="d6" />

    <!--  Definition of the graph  -->
    <graph edgedefault="undirected">
        <data key="d1">ServersBoxWithCentralRouter</data>

        <node id="0">
            <data key="d2">server</data>
            <data key="d3">10000</data>
            <data key="d4">50000</data>
        </node>
        <node id="1">
            <data key="d2">server</data>
            <data key="d3">5</data>
            <data key="d4">1</data>
        </node>
        <node id="2">
            <data key="d2">server</data>
            <data key="d3">5</data>
            <data key="d4">1</data>
        </node>
        <node id="3">
            <data key="d2">server</data>
            <data key="d3">2</data>
            <data key="d4">5</data>
        </node>
        <!-- central router -->
        <node id="4">
            <data key="d2">router</data>
        </node>

        <!-- perimeter edges -->
        <edge source="0" target="1">
            <data key="d5">1000</data>
            <data key="d6">5</data>
        </edge>
        <edge source="1" target="2">
            <data key="d5">1000</data>
            <data key="d6">5</data>
        </edge>
        <edge source="2" target="3">
            <data key="d5">1000</data>
            <data key="d6">5</data>
        </edge>
        <edge source="3" target="0">
            <data key="d5">1000</data>
            <data key="d6">5</data>
        </edge>
        <!-- edges from box's corners to central router -->
        <edge source="0" target="4">
            <data key="d5">1000</data>
            <data key="d6">5</data>
        </edge>
        <edge source="1" target="4">
            <data key="d5">1000</data>
            <data key="d6">5</data>
        </edge>
        <edge source="2" target="4">
            <data key="d5">1000</data>
            <data key="d6">5</data>
        </edge>
        <edge source="3" target="4">
            <data key="d5">1000</data>
            <data key="d6">5</data>
        </edge>

    </graph>
</graphml>